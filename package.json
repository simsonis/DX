{
  "name": "dx-project",
  "version": "1.0.0",
  "description": "Developer Experience toolkit and utilities",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write src/",
